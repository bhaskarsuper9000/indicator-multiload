"[ \"\", \"CPU $(percent(cpu.inuse))\", \"Mem $(size(mem.user))\", \"Net "
"$(speed(net.down))/$(speed(net.up))\", \"Swap $(size(swap.used))\", \"Load "
"$(decimals(load.avg,2))\", \"Disk $(speed(disk.read))/$(speed(disk.write))\" "
"]"
msgstr ""


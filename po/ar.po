"[ \"المعالجة: $(percent(cpu.inuse))، iowait $(percent(cpu.io))\", \"الذاكرة: "
"$(size(mem.user))، الخبيئة $(size(mem.cached))\", \"الشبكة: ينزل "
"$(speed(net.down))، يرفع $(speed(net.up))\", \"مساحة التبديل: "
"$(size(swap.used))\", \"الحِمل: $(decimals(load.avg,2))\", \"القرص الصلب: "
"يقرأ $(speed(disk.read))، يكتب $(speed(disk.write))\" ]"

